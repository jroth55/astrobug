---
export const prerender = false;
import BaseLayout from '../layouts/BaseLayout.astro';
const userAgent = Astro.request.headers.get('user-agent') || 'unknown';
---
<BaseLayout title="SSR Headers">
  <main class="container mx-auto px-4 py-16 space-y-4">
    <h1 class="text-2xl font-bold text-primary">SSR Headers</h1>
    <p class="text-foreground">User-Agent: {userAgent}</p>
  </main>
</BaseLayout>
